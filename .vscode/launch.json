{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flask Run",
            "type": "debugpy",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py",
                "FLASK_ENV": "development",
